{
  "name": "<%=packageName%>",
  "version": "0.0.1",
  "private": false,
  "files": [
    "lib",
    "package.json"
  ],
  "author": "<%=author%>",
  "description": "<%=description%>",
  "scripts": {
    "bootstrap": "yarn i",
    "dev": "vite",
    "serve": "vite preview",
    "clear": "rimraf ./lib && rimraf ./dist",
    "build": "cross-env NODE_ENV=production vite build && esno ./vite/script/postBuild.js",
    "build:no-cache": "yarn run clean:cache && yarn run build",
    "report": "cross-env REPORT=true yarn run build",
    "preview": "yarn run build && vite preview",
    "gen:package": "node scripts/gen-entry.js",
    "lib": "cross-env NODE_ENV=production rimraf ./lib && node scripts/rollup.config.js",
    "lint": "eslint --max-warnings 0  \"{src, packages, vite, scripts}/**/*.{vue, js, jsx}\" --fix",
    "stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
